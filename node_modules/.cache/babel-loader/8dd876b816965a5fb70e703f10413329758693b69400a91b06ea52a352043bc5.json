{"ast":null,"code":"import ProdutoItem from \"@/components/ProdutoItem.vue\";\nimport { api } from \"@/services.js\";\nimport { mapState } from \"vuex\";\nexport default {\n  components: {\n    ProdutoItem\n  },\n  data() {\n    return {\n      vendas: null\n    };\n  },\n  computed: {\n    ...mapState([\"usuario\", \"login\"])\n  },\n  methods: {\n    getVendas() {\n      api.get(`/transacao?tipo=vendedor_id`).then(response => {\n        this.vendas = response.data;\n      });\n    }\n  },\n  watch: {\n    login() {\n      this.getVendas();\n    }\n  },\n  created() {\n    if (this.login) {\n      this.getVendas();\n    }\n    document.title = \"Usuário | Vendas\";\n  }\n};","map":{"version":3,"names":["ProdutoItem","api","mapState","components","data","vendas","computed","methods","getVendas","get","then","response","watch","login","created","document","title"],"sources":["C:\\Users\\Thiago\\Desktop\\projetos\\vue\\ranek\\vue-ranek\\src\\views\\usuario\\UsuarioVendas.vue"],"sourcesContent":["<template>\r\n  <section>\r\n    <div v-if=\"vendas\">\r\n      <h2>Vendas</h2>\r\n      <div class=\"produtos-wrapper\" v-for=\"(venda, index) in vendas\" :key=\"index\">\r\n        <ProdutoItem v-if=\"venda.produto\" :produto=\"venda.produto\">\r\n          <p class=\"vendedor\">\r\n            <span>Comprador:</span>\r\n            {{venda.comprador_id}}\r\n          </p>\r\n        </ProdutoItem>\r\n        <div class=\"entrega\">\r\n          <h3>Entrega:</h3>\r\n          <ul v-if=\"venda.endereco\">\r\n            <li v-for=\"(value, key) in venda.endereco\" :key=\"key\">{{key}}: {{value}}</li>\r\n          </ul>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <PaginaCarregando v-else/>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport ProdutoItem from \"@/components/ProdutoItem.vue\";\r\nimport { api } from \"@/services.js\";\r\nimport { mapState } from \"vuex\";\r\n\r\nexport default {\r\n  components: {\r\n    ProdutoItem\r\n  },\r\n  data() {\r\n    return {\r\n      vendas: null\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState([\"usuario\", \"login\"])\r\n  },\r\n  methods: {\r\n    getVendas() {\r\n      api.get(`/transacao?tipo=vendedor_id`).then(response => {\r\n        this.vendas = response.data;\r\n      });\r\n    }\r\n  },\r\n  watch: {\r\n    login() {\r\n      this.getVendas();\r\n    }\r\n  },\r\n  created() {\r\n    if (this.login) {\r\n      this.getVendas();\r\n    }\r\n    document.title = \"Usuário | Vendas\";\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.produto-wrapper {\r\n  margin-bottom: 40px;\r\n}\r\n\r\n.vendedor span {\r\n  color: #e80;\r\n}\r\n\r\n.entrega {\r\n  display: grid;\r\n  grid-template-columns: minmax(100px, 200px) 1fr;\r\n  grid-gap: 20px;\r\n  margin-bottom: 60px;\r\n}\r\n\r\nh2 {\r\n  margin-bottom: 20px;\r\n}\r\n\r\nh3 {\r\n  margin: 0px;\r\n  justify-self: end;\r\n}\r\n\r\n@media screen and (max-width: 500px) {\r\n  .entrega {\r\n    grid-template-columns: 1fr;\r\n    grid-gap: 10px;\r\n  }\r\n  h3 {\r\n    justify-self: start;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AAwBA,OAAOA,WAAU,MAAO,8BAA8B;AACtD,SAASC,GAAE,QAAS,eAAe;AACnC,SAASC,QAAO,QAAS,MAAM;AAE/B,eAAe;EACbC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGJ,QAAQ,CAAC,CAAC,SAAS,EAAE,OAAO,CAAC;EAClC,CAAC;EACDK,OAAO,EAAE;IACPC,SAASA,CAAA,EAAG;MACVP,GAAG,CAACQ,GAAG,CAAE,6BAA4B,CAAC,CAACC,IAAI,CAACC,QAAO,IAAK;QACtD,IAAI,CAACN,MAAK,GAAIM,QAAQ,CAACP,IAAI;MAC7B,CAAC,CAAC;IACJ;EACF,CAAC;EACDQ,KAAK,EAAE;IACLC,KAAKA,CAAA,EAAG;MACN,IAAI,CAACL,SAAS,EAAE;IAClB;EACF,CAAC;EACDM,OAAOA,CAAA,EAAG;IACR,IAAI,IAAI,CAACD,KAAK,EAAE;MACd,IAAI,CAACL,SAAS,EAAE;IAClB;IACAO,QAAQ,CAACC,KAAI,GAAI,kBAAkB;EACrC;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}