{"ast":null,"code":"import ProdutosPaginar from \"@/components/ProdutosPaginar.vue\";\nimport { api } from \"@/services.js\";\nimport { serialize } from \"@/helpers.js\";\nexport default {\n  name: \"ProdutosLista\",\n  components: {\n    ProdutosPaginar\n  },\n  data() {\n    return {\n      produtos: null,\n      produtosPorPagina: 9,\n      produtosTotal: 0\n    };\n  },\n  computed: {\n    url() {\n      const query = serialize(this.$route.query);\n      return `/produto?_limit=${this.produtosPorPagina}${query}`;\n    }\n  },\n  methods: {\n    getProdutos() {\n      this.produtos = null;\n      api.get(this.url).then(response => {\n        this.produtosTotal = Number(response.headers[\"x-total-count\"]);\n        this.produtos = response.data;\n      });\n    }\n  },\n  watch: {\n    url() {\n      this.getProdutos();\n    }\n  },\n  created() {\n    this.getProdutos();\n  }\n};","map":{"version":3,"names":["ProdutosPaginar","api","serialize","name","components","data","produtos","produtosPorPagina","produtosTotal","computed","url","query","$route","methods","getProdutos","get","then","response","Number","headers","watch","created"],"sources":["C:\\Users\\Thiago\\Desktop\\projetos\\vue\\ranek\\vue-ranek\\src\\components\\ProdutosLista.vue"],"sourcesContent":["<template>\r\n  <section class=\"produtos-container\">\r\n    <transition mode=\"out-in\">\r\n      <div v-if=\"produtos && produtos.length\" class=\"produtos\" key=\"produtos\">\r\n        <div class=\"produto\" v-for=\"(produto, index) in produtos\" :key=\"index\">\r\n          <router-link :to=\"{name: 'produto', params: {id: produto.id}}\">\r\n            <img v-if=\"produto.fotos\" :src=\"produto.fotos[0].src\" :alt=\"produto.fotos[0].titulo\">\r\n            <p class=\"preco\">{{produto.preco || numeroPreco}}</p>\r\n            <h2 class=\"titulo\">{{produto.nome}}</h2>\r\n            <p>{{produto.descricao}}</p>\r\n          </router-link>\r\n        </div>\r\n        <ProdutosPaginar :produtosTotal=\"produtosTotal\" :produtosPorPagina=\"produtosPorPagina\"/>\r\n      </div>\r\n      <div v-else-if=\"produtos && produtos.length === 0\" key=\"sem-resultados\">\r\n        <p class=\"sem-resultados\">Busca sem resultados. Tente buscar outro termo.</p>\r\n      </div>\r\n      <PaginaCarregando key=\"carregando\" v-else/>\r\n    </transition>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport ProdutosPaginar from \"@/components/ProdutosPaginar.vue\";\r\nimport { api } from \"@/services.js\";\r\nimport { serialize } from \"@/helpers.js\";\r\n\r\nexport default {\r\n  name: \"ProdutosLista\",\r\n  components: {\r\n    ProdutosPaginar\r\n  },\r\n  data() {\r\n    return {\r\n      produtos: null,\r\n      produtosPorPagina: 9,\r\n      produtosTotal: 0\r\n    };\r\n  },\r\n  computed: {\r\n    url() {\r\n      const query = serialize(this.$route.query);\r\n      return `/produto?_limit=${this.produtosPorPagina}${query}`;\r\n    }\r\n  },\r\n  methods: {\r\n    getProdutos() {\r\n      this.produtos = null;\r\n      api.get(this.url).then(response => {\r\n        this.produtosTotal = Number(response.headers[\"x-total-count\"]);\r\n        this.produtos = response.data;\r\n      });\r\n    }\r\n  },\r\n  watch: {\r\n    url() {\r\n      this.getProdutos();\r\n    }\r\n  },\r\n  created() {\r\n    this.getProdutos();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.produtos-container {\r\n  max-width: 1000px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.produtos {\r\n  display: grid;\r\n  grid-template-columns: repeat(3, 1fr);\r\n  grid-gap: 30px;\r\n  margin: 30px;\r\n}\r\n\r\n@media screen and (max-width: 500px) {\r\n  .produtos {\r\n    grid-template-columns: repeat(2, 1fr);\r\n    grid-gap: 10px;\r\n    margin: 10px;\r\n  }\r\n}\r\n\r\n.produto {\r\n  box-shadow: 0 4px 8px rgba(30, 60, 90, 0.1);\r\n  padding: 10px;\r\n  background: #fff;\r\n  border-radius: 4px;\r\n  transition: all 0.2s;\r\n}\r\n\r\n.produto:hover {\r\n  box-shadow: 0 6px 12px rgba(30, 60, 90, 0.2);\r\n  transform: scale(1.1);\r\n  position: relative;\r\n  z-index: 1;\r\n}\r\n\r\n.produto img {\r\n  border-radius: 4px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.titulo {\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.preco {\r\n  color: #e80;\r\n  font-weight: bold;\r\n}\r\n\r\n.sem-resultados {\r\n  text-align: center;\r\n}\r\n</style>\r\n"],"mappings":"AAuBA,OAAOA,eAAc,MAAO,kCAAkC;AAC9D,SAASC,GAAE,QAAS,eAAe;AACnC,SAASC,SAAQ,QAAS,cAAc;AAExC,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVJ;EACF,CAAC;EACDK,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,IAAI;MACdC,iBAAiB,EAAE,CAAC;MACpBC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,GAAGA,CAAA,EAAG;MACJ,MAAMC,KAAI,GAAIT,SAAS,CAAC,IAAI,CAACU,MAAM,CAACD,KAAK,CAAC;MAC1C,OAAQ,mBAAkB,IAAI,CAACJ,iBAAkB,GAAEI,KAAM,EAAC;IAC5D;EACF,CAAC;EACDE,OAAO,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACR,QAAO,GAAI,IAAI;MACpBL,GAAG,CAACc,GAAG,CAAC,IAAI,CAACL,GAAG,CAAC,CAACM,IAAI,CAACC,QAAO,IAAK;QACjC,IAAI,CAACT,aAAY,GAAIU,MAAM,CAACD,QAAQ,CAACE,OAAO,CAAC,eAAe,CAAC,CAAC;QAC9D,IAAI,CAACb,QAAO,GAAIW,QAAQ,CAACZ,IAAI;MAC/B,CAAC,CAAC;IACJ;EACF,CAAC;EACDe,KAAK,EAAE;IACLV,GAAGA,CAAA,EAAG;MACJ,IAAI,CAACI,WAAW,EAAE;IACpB;EACF,CAAC;EACDO,OAAOA,CAAA,EAAG;IACR,IAAI,CAACP,WAAW,EAAE;EACpB;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}