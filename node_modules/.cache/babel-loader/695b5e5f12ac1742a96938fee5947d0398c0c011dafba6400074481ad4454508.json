{"ast":null,"code":"import { api } from \"@/services.js\";\nimport FinalizarCompra from \"@/components/FinalizarCompra.vue\";\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: 'Produtos',\n  props: [\"id\"],\n  components: {\n    FinalizarCompra\n  },\n  data() {\n    return {\n      produto: null,\n      finalizar: false\n    };\n  },\n  methods: {\n    getProduto() {\n      api.get(`/produto/${this.id}`).then(response => {\n        this.produto = response.data;\n        document.title = this.produto.nome;\n      });\n    }\n  },\n  created() {\n    this.getProduto();\n  }\n};","map":{"version":3,"names":["api","FinalizarCompra","name","props","components","data","produto","finalizar","methods","getProduto","get","id","then","response","document","title","nome","created"],"sources":["C:\\Users\\Thiago\\Desktop\\projetos\\vue\\ranek\\vue-ranek\\src\\views\\Produto.vue"],"sourcesContent":["<template>\r\n  <section>\r\n    <div v-if=\"produto\" class=\"produto\">\r\n      <ul class=\"fotos\" v-if=\"produto.fotos\">\r\n        <li v-for=\"(foto, index) in produto.fotos\" :key=\"index\">\r\n          <img :src=\"foto.src\" :alt=\"foto.titulo\">\r\n        </li>\r\n      </ul>\r\n      <div class=\"info\">\r\n        <h1>{{produto.nome}}</h1>\r\n        <p class=\"preco\">{{produto.preco || numeroPreco}}</p>\r\n        <p class=\"descricao\">{{produto.descricao}}</p>\r\n        <transition mode=\"out-in\" v-if=\"produto.vendido === 'false'\">\r\n          <button class=\"btn\" v-if=\"!finalizar\" @click=\"finalizar = true\">Comprar</button>\r\n          <FinalizarCompra v-else :produto=\"produto\"/>\r\n        </transition>\r\n        <button v-else class=\"btn btn-disabled\" disabled>Produto Vendido</button>\r\n      </div>\r\n    </div>\r\n    <PaginaCarregando v-else/>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport { api } from \"@/services.js\";\r\nimport FinalizarCompra from \"@/components/FinalizarCompra.vue\";\r\n\r\nexport default {\r\n  // eslint-disable-next-line vue/multi-word-component-names\r\n  name: 'Produtos',\r\n  props: [\"id\"],\r\n  components: {\r\n    FinalizarCompra\r\n  },\r\n  data() {\r\n    return {\r\n      produto: null,\r\n      finalizar: false\r\n    };\r\n  },\r\n  methods: {\r\n    getProduto() {\r\n      api.get(`/produto/${this.id}`).then(response => {\r\n        this.produto = response.data;\r\n        document.title = this.produto.nome;\r\n      });\r\n    }\r\n  },\r\n  created() {\r\n    this.getProduto();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.produto {\r\n  display: grid;\r\n  grid-template-columns: 1fr 1fr;\r\n  grid-gap: 30px;\r\n  max-width: 900px;\r\n  padding: 60px 20px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.preco {\r\n  color: #e80;\r\n  font-weight: bold;\r\n  font-size: 1.5rem;\r\n  margin-bottom: 40px;\r\n}\r\n\r\n.fotos {\r\n  grid-row: 1 / 3;\r\n}\r\n\r\n.info {\r\n  position: sticky;\r\n  top: 20px;\r\n}\r\n\r\n.descricao {\r\n  font-size: 1.2rem;\r\n}\r\n\r\nimg {\r\n  margin-bottom: 30px;\r\n  box-shadow: 0 4px 8px rgba(30, 60, 90, 0.2);\r\n  border-radius: 4px;\r\n}\r\n\r\n.btn {\r\n  margin-top: 60px;\r\n  width: 200px;\r\n}\r\n\r\n@media screen and (max-width: 500px) {\r\n  .produto {\r\n    grid-template-columns: 1fr;\r\n  }\r\n  .fotos {\r\n    grid-row: 2;\r\n  }\r\n  .info {\r\n    position: initial;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AAwBA,SAASA,GAAE,QAAS,eAAe;AACnC,OAAOC,eAAc,MAAO,kCAAkC;AAE9D,eAAe;EACb;EACAC,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE,CAAC,IAAI,CAAC;EACbC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,IAAI;MACbC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACXT,GAAG,CAACU,GAAG,CAAE,YAAW,IAAI,CAACC,EAAG,EAAC,CAAC,CAACC,IAAI,CAACC,QAAO,IAAK;QAC9C,IAAI,CAACP,OAAM,GAAIO,QAAQ,CAACR,IAAI;QAC5BS,QAAQ,CAACC,KAAI,GAAI,IAAI,CAACT,OAAO,CAACU,IAAI;MACpC,CAAC,CAAC;IACJ;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACR,UAAU,EAAE;EACnB;AACF,CAAC"},"metadata":{},"so